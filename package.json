{
  "name": "slack.fm",
  "version": "1.0.0",
  "description": "last.fm bot for slack",
  "main": "src/index.js",
  "type": "module",
  "imports": {
    "#utils/*": "./src/utils/*",
    "#commands/*": "./src/commands/*"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint src/ && prettier -c src/",
    "lint:fix": "eslint src/ --fix && prettier --write src/",
    "format": "prettier --write src/"
  },
  "keywords": [],
  "author": "BananaJeans",
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^4.4.0",
    "axios": "^1.12.2",
    "bad-words": "^4.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "md5": "^2.3.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.5.3"
  }
}
